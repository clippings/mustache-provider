{
    "name": "clippings/mustache-provider",
    "description": "A Mustache service provider for Pimple and Silex.",
    "keywords": ["templating", "mustache", "silex", "pimple"],
    "homepage": "https://github.com/clippings/mustache-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Hileman",
            "email": "justin@justinhileman.info",
            "homepage": "http://justinhileman.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "mustache/mustache": "^2.0"
    },
    "require-dev": {
        "silex/silex": "^2.0"
    },
    "autoload": {
        "psr-4": { "Mustache\\": "src/" }
    },
    "replace": {
        "mustache/silex-provider": "*"
    }
}
